<table id="new_services">
  <thead>
    <tr>
      <th>Title</th>
      <th>Code</th>
      <th>Status</th>
      <th colspan='3'></th>
    </tr>
  </thead>

	<tbody>
		<% @services.each do |service| %>
	  <tr>
		  <td><%= service.title %></td>
		  <td><%= service.code %></td>
		  <td><%= service.status %></td>
		  <td>
		    <%= link_to "Show", service %>
		    <%= link_to "Edit", edit_service_path(service) %>
		    <%= link_to "Destroy", service, method: :delete, confirm: 'Are you sure?', remote: true %>
		  </td>
		</tr>
		<% end %>
	</tbody>
</table>